             tInsertDocs4110.cOtpEd := getCreateOtpEd(tInsertDocs4110.cmc, tInsertDocs4110.edIzm1C, tInsertDocs4110.edIzm_koeff);
             tInsertDocs4110.kolfordoc := tInsertDocs4110.kol1C;

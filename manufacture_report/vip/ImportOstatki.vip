function getOtpEdbyName(_cmc : comp; _edIzm : string) : comp;
{
   var _ced : comp = getKatEd(_edIzm);
   if getfirst KatOtpEd where ((1 == KatOtpEd.prmc
                   and katmc.nrec == KatOtpEd.cMcUsl
                        and _cEd  == KatOtpEd.CKATED)) = tsOk {

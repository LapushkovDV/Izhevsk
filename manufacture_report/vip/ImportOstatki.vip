    set _cGrPartyGOZ := GROUPPARTY.nrec
   }
 else {
   message('Не найдена группа партий с кодом "ГОЗ" и наименованием "Исполнение гос.заказов"'+
 +    ''#13'' + 'Работа прекращена',error);

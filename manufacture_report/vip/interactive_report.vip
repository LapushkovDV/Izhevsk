                          tspmnplan.cMc   as cmc
                        , tspmnplan.corg  as corg
                        , tspmnplan.cPodr as cpodr
                        , indent.dPlan    as zakazDate
                        , claim.nrec      as claimnrec
                        , KatZak.NomZak   as ZakazNumberAvto

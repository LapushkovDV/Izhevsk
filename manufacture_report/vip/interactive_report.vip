         sql
           select
               t.cMc
             , t.corg
             , t.cPodr
             , t.zakazDate
             , '' as ZakazNumber
             , t.ZakazNumberAvto
             , sum(SpObjAcc.kolcpos) as KolZakazAvto
           from
            (

SQL Query qrDelete_SpObjAcc =
DELETE SpObjAcc
WHERE
SpObjAcc.Nrec IN
 (
  SELECT %BatchSize
     a.Nrec
  FROM
     ObjAccT
  JOIN SpObjAcc a ON
  (
     ObjAccT.Nrec = a.cObjAccT
  )
  WHERE
   (
     ObjAccT.%FldOwnerType = %wTypePlan AND
     ObjAccT.%FldOwnerNrec = %cMnPlanNrec AND
     ObjAccT.KindRec = 1
   )
 );

SQL Query qrDelete_ValSpMnP =
DELETE ValSpMnP
WHERE
  ValSpMnP.Nrec IN
   (
     SELECT %BatchSize
       a.nrec
     FROM
       SpMnPlan
     JOIN SpMnPl ON
      (
         SpMnPlan.Nrec = SpMnPl.cSpMnPlan
         %DocView_Condition
      )
     JOIN ValSpMnP a ON
      (
         SpMnPl.Nrec = a.cSpMnPl
      )
     WHERE
         SpMnPlan.cMnPlan = %cMnPlanNrec
    );


SQL Query qrDelete_PropSpMn =
DELETE PropSpMn
WHERE
 PropSpMn.cSpMnPl IN
   (
     SELECT %BatchSize
       a.nrec
     FROM
       SpMnPlan
     JOIN SpMnPl a ON
     (
        SpMnPlan.Nrec = a.cSpMnPlan
        %DocView_Condition
     )
     WHERE
       SpMnPlan.cMnPlan = %cMnPlanNrec
  );

SQL Query qrDelete_SpMnPl =
DELETE SpMnPl
WHERE
  SpMnPl.Nrec IN
   (
     SELECT %BatchSize
       a.nrec
     FROM
       SpMnPlan
     JOIN SpMnPl a ON
     (
        SpMnPlan.Nrec = a.cSpMnPlan
        %DocView_Condition
     )
     WHERE
       SpMnPlan.cMnPlan = %cMnPlanNrec
  ) ;

SQL Query qrDelete_SpMnPlan =
DELETE SpMnPlan
WHERE
  SpMnPlan.Nrec IN
   (
      SELECT %BatchSize
        SpMnPlan.Nrec
      FROM
        MnPlan
      WHERE
        (
          SpMnPlan.cMnPlan = %cMnPlanNrec
        )
   ) ;

// Получить будущую спецификацию плана снабжения на основе данных таблицы в памяти TmpSpObjAcc
SQL Query qrGetFutureSpecPlanSnab =
  Select DISTINCT
    TmpSpObjAcc.cProduct        as cIzd
  FROM
    TmpSpObjAcc
  WHERE
    TmpSpObjAcc.tProduct = :typeIzd
;

